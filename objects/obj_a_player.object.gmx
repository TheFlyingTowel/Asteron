            <string>///Simple Movement in step event 

//(Initializing movement variables)
var MoveLeft = keyboard_check(vk_left);
var MoveRight = keyboard_check(vk_right);
var MoveUp = keyboard_check(vk_up);
var MoveDown = keyboard_check(vk_down);

//Move left

if (MoveLeft) {
 phy_position_x -= mspd;
 sprite_index = spr_a_player_left;
 image_speed = .2
}

//Move right

if (MoveRight) {
 phy_position_y += mspd;
 sprite_index = spr_a_player_right;
 image_speed = .2
}

//Move up

if (MoveUp) {
 phy_position_y -= mspd;
 sprite_index = spr_a_player_up;
 image_speed = .2
}

//Move down

if (MoveDown) {
 phy_position_y += mspd;
 sprite_index = spr_a_player_down;
 image_speed = .2
}

//stop player animation

if (!MoveDown and !MoveUp and !MoveLeft and !MoveRight ) {
image_speed = 0;
image_index = 0;
}


</string>
